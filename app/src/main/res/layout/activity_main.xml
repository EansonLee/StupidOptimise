<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:toos="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/tv_get_tid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="获取tid，并设置亲和性" />

    <Button
        android:id="@+id/tv_get_affinity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="获取亲和性" />

    <Button
        android:id="@+id/tv_set_big_affinity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="绑定大核" />

    <Button
        android:id="@+id/tv_hook_lib"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="hook_native" />

    <Button
        android:id="@+id/tv_gc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="tv_gc" />
</LinearLayout>